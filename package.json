{
	"name": "sveltekit-webpool",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --https",
		"build": "npx rimraf public && svelte-kit build --verbose",
		"preview-build-locally": "svelte-kit preview --port 5000 --https",
		"deploy": "firebase deploy",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"host-firebase-preview-channel": "firebase hosting:channel:deploy preview-test",
		"clone-preview-channel-to-live": "firebase hosting:clone tonyswebpool:preview-test tonyswebpool:live"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.0.0-next.168",
		"@types/cookie": "^0.4.1",
		"@types/uuid": "^8.3.1",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"@zerodevx/svelte-toast": "^0.6.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss": "^8.3.8",
		"postcss-load-config": "^3.1.0",
		"postcss-scss": "^4.0.1",
		"prettier": "~2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.43.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.5",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite-imagetools": "^3.6.8"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^5.15.4",
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"autoprefixer": "github:postcss/autoprefixer",
		"cookie": "^0.4.1",
		"dialog-polyfill": "^0.5.6",
		"firebase": "9.1",
		"firebase-admin": "^9.12.0",
		"firebase-functions": "^3.15.7",
		"firebase-tools": "^9.19.0",
		"kit": "github:sveltejs/kit",
		"sass": "^1.42.1",
		"svelte-adapter-firebase": "0.13.0",
		"svelte-fa": "^2.3.3",
		"svelte-intersection-observer": "^0.8.0",
		"svelte-simple-modal": "^1.0.3",
		"uuid": "^8.3.2",
		"vite-plugin-mkcert": "^1.5.1"
	}
}
